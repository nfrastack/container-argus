#!/command/with-contenv bash

ADMIN_PASS=${ADMIN_PASS:-"null"}
ADMIN_USER=${ADMIN_USER:-"null"}
CONFIG_FILE=${CONFIG_FILE:-"argus.yaml"}
CONFIG_PATH=${CONFIG_PATH:-"/config/"}
DATA_FILE=${DATA_FILE:-"argus.db"}
DATA_PATH=${DATA_PATH:-"/data/"}
LISTEN_IP=${LISTEN_IP:-"0.0.0.0"}
LISTEN_PORT=${LISTEN_PORT:-"8080"}
LISTEN_PREFIX=${LISTEN_PREFIX:-"/"}
LOG_FILE=${LOG_FILE:-"argus.log"}
LOG_LEVEL=${LOG_LEVEL:-"INFO"}
LOG_PATH=${LOG_PATH:-"/logs/"}
LOG_TYPE=${LOG_TYPE:-"CONSOLE"}
SETUP_TYPE=${SETUP_TYPE:-"AUTO"}
RESTART_ON_CONFIG_CHANGE=${RESTART_ON_CONFIG_CHANGE:-"TRUE"}